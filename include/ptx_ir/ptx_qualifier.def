// =============================================================================
// PTX Qualifier Type Definition (X-Macro Format)
// Format: X(enum_value, enum_name, string_value)
// Note: Defines all possible qualifiers in PTX
// =============================================================================

// -----------------------------------------------------------------------------
// 1. Data Types
// -----------------------------------------------------------------------------
X(Q_U8, U8, ".u8")      // Unsigned 8-bit integer
X(Q_U16, U16, ".u16")   // Unsigned 16-bit integer
X(Q_U32, U32, ".u32")   // Unsigned 32-bit integer
X(Q_U64, U64, ".u64")   // Unsigned 64-bit integer

X(Q_S8, S8, ".s8")      // Signed 8-bit integer
X(Q_S16, S16, ".s16")   // Signed 16-bit integer
X(Q_S32, S32, ".s32")   // Signed 32-bit integer
X(Q_S64, S64, ".s64")   // Signed 64-bit integer

X(Q_B8, B8, ".b8")      // Bit 8-bit
X(Q_B16, B16, ".b16")   // Bit 16-bit
X(Q_B32, B32, ".b32")   // Bit 32-bit
X(Q_B64, B64, ".b64")   // Bit 64-bit

X(Q_F8, F8, ".f8")      // Float 8-bit
X(Q_F16, F16, ".f16")   // Float 16-bit
X(Q_F32, F32, ".f32")   // Float 32-bit
X(Q_F64, F64, ".f64")   // Float 64-bit

X(Q_PRED, PRED, ".pred") // Predicate

// -----------------------------------------------------------------------------
// 2. Vector Types
// -----------------------------------------------------------------------------
X(Q_V2, V2, ".v2")      // Vector of 2 elements
X(Q_V4, V4, ".v4")      // Vector of 4 elements

// -----------------------------------------------------------------------------
// 3. Memory Space Qualifiers
// -----------------------------------------------------------------------------
X(Q_CONST, CONST, ".const")    // Constant memory space
X(Q_PARAM, PARAM, ".param")    // Parameter space
X(Q_GLOBAL, GLOBAL, ".global") // Global memory space
X(Q_LOCAL, LOCAL, ".local")    // Local memory space
X(Q_SHARED, SHARED, ".shared") // Shared memory space

// -----------------------------------------------------------------------------
// 4. Pointer Type Qualifier
// -----------------------------------------------------------------------------
X(Q_PTR, PTR, ".ptr")    // Pointer type

// -----------------------------------------------------------------------------
// 5. Comparison and Condition Qualifiers
// -----------------------------------------------------------------------------
X(Q_GT, GT, ".gt")      // Greater than
X(Q_GE, GE, ".ge")      // Greater equal
X(Q_EQ, EQ, ".eq")      // Equal
X(Q_NE, NE, ".ne")      // Not equal
X(Q_LT, LT, ".lt")      // Less than
X(Q_LE, LE, ".le")      // Less equal
X(Q_LTU, LTU, ".ltu")   // Less than unsigned
X(Q_LEU, LEU, ".leu")   // Less equal unsigned
X(Q_GTU, GTU, ".gtu")   // Greater than unsigned
X(Q_GEU, GEU, ".geu")   // Greater equal unsigned

// -----------------------------------------------------------------------------
// 6. Rounding Modifiers
// -----------------------------------------------------------------------------

// Integer Rounding Modifiers
X(Q_RNI, RNI, ".rni")   // Round to nearest integer, ties to even
X(Q_RZI, RZI, ".rzi")   // Round toward zero (truncate)
X(Q_RMI, RMI, ".rmi")   // Round toward negative infinity
X(Q_RPI, RPI, ".rpi")   // Round toward positive infinity

// Floating-Point Rounding Modifiers
X(Q_RN, RN, ".rn")      // Round to nearest even
X(Q_RNA, RNA, ".rna")   // Round to nearest, ties away from zero
X(Q_RZ, RZ, ".rz")      // Round toward zero
X(Q_RM, RM, ".rm")      // Round toward -∞
X(Q_RP, RP, ".rp")      // Round toward +∞
X(Q_RS, RS, ".rs")       // Stochastic rounding

// -----------------------------------------------------------------------------
// 7. Other Qualifiers
// -----------------------------------------------------------------------------
X(Q_TO, TO, ".to")      // To
X(Q_WIDE, WIDE, ".wide") // Wide
X(Q_SYNC, SYNC, ".sync") // Synchronous
X(Q_LO, LO, ".lo")      // Low part
X(Q_HI, HI, ".hi")      // High part
X(Q_A, A, ".a")         // A operand
X(Q_B, B, ".b")         // B operand
X(Q_D, D, ".d")         // D operand
X(Q_ROW, ROW, ".row")   // Row

// -----------------------------------------------------------------------------
// 8. Alignment and Special Qualifiers
// -----------------------------------------------------------------------------
X(Q_ALIGNED, ALIGNED, ".aligned") // Aligned
X(Q_M8N8K4, M8N8K4, ".m8n8k4")   // Matrix size 8x8x4
X(Q_M16N16K16, M16N16K16, ".m16n16k16") // Matrix size 16x16x16

// -----------------------------------------------------------------------------
// 9. WMMA and Specialized Qualifiers
// -----------------------------------------------------------------------------
X(Q_NEU, NEU, ".neu")    // Neu
X(Q_NC, NC, ".nc")      // No cache
X(Q_FTZ, FTZ, ".ftz")   // Flush to zero
X(Q_APPROX, APPROX, ".approx") // Approximate
X(Q_DOTADD, DOTADD, ".add")    // Dot add
X(Q_DOTOR, DOTOR, ".or")       // Dot or
X(Q_SAT, SAT, ".sat")   // Saturation

// -----------------------------------------------------------------------------
// 10. New qualifier: Carry Condition (for addc/subc)
// -----------------------------------------------------------------------------
X(Q_CC, CC, ".cc")      // Carry condition

// -----------------------------------------------------------------------------
// 11. FP8 Types (PTX 8.7+)
// -----------------------------------------------------------------------------
X(Q_E4M3, E4M3, ".e4m3")
X(Q_E5M2, E5M2, ".e5m2")
X(Q_E3M2, E3M2, ".e3m2")
X(Q_E2M3, E2M3, ".e2m3")
X(Q_E2M1, E2M1, ".e2m1")

// Packed vector FP8
X(Q_E4M3X4, E4M3X4, ".e4m3x4")
X(Q_E5M2X4, E5M2X4, ".e5m2x4")
X(Q_E3M2X4, E3M2X4, ".e3m2x4")
X(Q_E2M3X4, E2M3X4, ".e2m3x4")
X(Q_E2M1X4, E2M1X4, ".e2m1x4")

// -----------------------------------------------------------------------------
// 12. Async & Memory Ordering Qualifiers (PTX 8.7+)
// -----------------------------------------------------------------------------
X(Q_MMIO, MMIO, ".mmio")
X(Q_RELEASE, RELEASE, ".release")
X(Q_ACQUIRE, ACQUIRE, ".acquire")
X(Q_CLUSTER, CLUSTER, ".cluster")   // scope qualifier
X(Q_CTA, CTA, ".cta")               // scope qualifier
X(Q_LEVEL_CACHE_HINT, LEVEL_CACHE_HINT, ".level::cache_hint")

// -----------------------------------------------------------------------------
// 13. WMMA Kind and Scale (PTX 8.7+)
// -----------------------------------------------------------------------------
X(Q_KIND_MXF4NVF4, KIND_MXF4NVF4, ".kind::mxf4nvf4")
X(Q_SCALE_VEC_SIZE, SCALE_VEC_SIZE, ".scale_vec_size")
X(Q_BLOCK_SCALE, BLOCK_SCALE, ".block_scale")

// -----------------------------------------------------------------------------
// 14. New Shapes
// -----------------------------------------------------------------------------
X(Q_M32N8K16, M32N8K16, ".m32n8k16")
X(Q_M16N8K16, M16N8K16, ".m16n8k16")

// -----------------------------------------------------------------------------
// 15. Cache Levels
// -----------------------------------------------------------------------------
X(Q_L1, L1, ".l1")
X(Q_L2, L2, ".l2")

// -----------------------------------------------------------------------------
// 16. Membar Scopes
// -----------------------------------------------------------------------------
X(Q_CTX, CTX, ".ctx")
X(Q_SYS, SYS, ".sys")
X(Q_COHERENT, COHERENT, ".coherent")
X(Q_STRONG, STRONG, ".strong")

// -----------------------------------------------------------------------------
// 17. Vote Modes
// -----------------------------------------------------------------------------
X(Q_BALLOT, BALLOT, ".ballot")
X(Q_ANY, ANY, ".any")
X(Q_ALL, ALL, ".all")
X(Q_UNI, UNI, ".uni")
X(Q_EQV, EQV, ".eqv")

// -----------------------------------------------------------------------------
// 18. Shuffle Modes
// -----------------------------------------------------------------------------
X(Q_UP, UP, ".up")
X(Q_DOWN, DOWN, ".down")
X(Q_BFLY, BFLY, ".bfly")
X(Q_IDX, IDX, ".idx")